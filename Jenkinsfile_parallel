pipeline {
    agent any
    stages {
        stage(“Compile & Build Binary”) {
            parallel {
                stage(“dockerbuild”) {
				steps {
					sh """
					echo "build"
					"""
					}
				}
            
                stage(“dockerbuild”) {
     			steps {
					sh """
					echo "build"
					"""
					}
                }
			}
        }
   
   stage('docker build/push') {            
   sh """
   echo "push image"
   """
   }     
   }
}
