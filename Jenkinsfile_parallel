pipeline {
    agent any
    stages {
        stage('Compile and Build Binary') {
            parallel {
                stage(“dockerbuild”) {
				steps {
					sh """
					echo "build"
					"""
					}
				}
            
                stage(“sonar”) {
     			steps {
					sh """
					echo "sonar"
					"""
					}
                }
			}
        }
   
	   stage('docker build/push') {            
			steps {
			   sh """
				echo "push image"
				"""
			}
	   }     
	}
}
